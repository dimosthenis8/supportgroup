const express = require('express')
const app = express()
const port = 8765

app.get('/energy/api/ActualTotalLoad', (req, res) => {
	res.send('ActualTotalLoad')
});

app.get('/energy/api/DayAheadTotalLoadForecast', (req, res) => {
	res.send('DayAheadTotalLoadForecast')
});

app.get('/energy/api/AggregatedGenerationPerType', (req, res) => {
	res.send('AggregatedGenerationPerType')
});

app.listen(port, () => console.log(`Example app listening on port ${port}!`))
